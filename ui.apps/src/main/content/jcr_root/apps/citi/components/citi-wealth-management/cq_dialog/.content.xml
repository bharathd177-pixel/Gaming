<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Citi Wealth Management"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <section
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <sectionTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Section Title"
                                        name="./sectionTitle"/>
                                    <sectionDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Section Description"
                                        name="./sectionDescription"/>
                                </items>
                            </section>
                        </items>
                    </general>
                    <card1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <card1Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card 1 Title"
                                name="./card1Title"/>
                            <card1Description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Card 1 Description"
                                name="./card1Description"/>
                            <card1IconImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Card 1 Icon Image"
                                fileNameParameter="./card1IconImage"
                                fileReferenceParameter="./card1IconImageRef"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./card1IconImage"
                                title="Upload Card 1 Icon"/>
                            <card1IconAlt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card 1 Icon Alt Text"
                                name="./card1IconAlt"/>
                            <card1Link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Card 1 Link"
                                name="./card1Link"
                                rootPath="/content"/>
                        </items>
                    </card1>
                    <card2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <card2Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card 2 Title"
                                name="./card2Title"/>
                            <card2Description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Card 2 Description"
                                name="./card2Description"/>
                            <card2IconImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Card 2 Icon Image"
                                fileNameParameter="./card2IconImage"
                                fileReferenceParameter="./card2IconImageRef"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./card2IconImage"
                                title="Upload Card 2 Icon"/>
                            <card2IconAlt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card 2 Icon Alt Text"
                                name="./card2IconAlt"/>
                            <card2Link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Card 2 Link"
                                name="./card2Link"
                                rootPath="/content"/>
                        </items>
                    </card2>
                    <card3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Card 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <card3Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card 3 Title"
                                name="./card3Title"/>
                            <card3Description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Card 3 Description"
                                name="./card3Description"/>
                            <card3IconImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Card 3 Icon Image"
                                fileNameParameter="./card3IconImage"
                                fileReferenceParameter="./card3IconImageRef"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./card3IconImage"
                                title="Upload Card 3 Icon"/>
                            <card3IconAlt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card 3 Icon Alt Text"
                                name="./card3IconAlt"/>
                            <card3Link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Card 3 Link"
                                name="./card3Link"
                                rootPath="/content"/>
                        </items>
                    </card3>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
