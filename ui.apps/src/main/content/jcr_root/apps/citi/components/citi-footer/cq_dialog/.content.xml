<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Citi Footer"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Column 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column1Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 1 Title"
                                name="./column1Title"/>
                            <column1Link1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 1 Link 1"
                                name="./column1Link1"
                                rootPath="/content"/>
                            <column1Text1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 1 Text 1"
                                name="./column1Text1"/>
                            <column1Link2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 1 Link 2"
                                name="./column1Link2"
                                rootPath="/content"/>
                            <column1Text2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 1 Text 2"
                                name="./column1Text2"/>
                            <column1Link3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 1 Link 3"
                                name="./column1Link3"
                                rootPath="/content"/>
                            <column1Text3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 1 Text 3"
                                name="./column1Text3"/>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Column 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column2Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 2 Title"
                                name="./column2Title"/>
                            <column2Link1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 2 Link 1"
                                name="./column2Link1"
                                rootPath="/content"/>
                            <column2Text1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 2 Text 1"
                                name="./column2Text1"/>
                            <column2Link2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 2 Link 2"
                                name="./column2Link2"
                                rootPath="/content"/>
                            <column2Text2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 2 Text 2"
                                name="./column2Text2"/>
                            <column2Link3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 2 Link 3"
                                name="./column2Link3"
                                rootPath="/content"/>
                            <column2Text3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 2 Text 3"
                                name="./column2Text3"/>
                        </items>
                    </column2>
                    <column3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Column 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column3Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 3 Title"
                                name="./column3Title"/>
                            <column3Link1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 3 Link 1"
                                name="./column3Link1"
                                rootPath="/content"/>
                            <column3Text1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 3 Text 1"
                                name="./column3Text1"/>
                            <column3Link2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 3 Link 2"
                                name="./column3Link2"
                                rootPath="/content"/>
                            <column3Text2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 3 Text 2"
                                name="./column3Text2"/>
                            <column3Link3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Column 3 Link 3"
                                name="./column3Link3"
                                rootPath="/content"/>
                            <column3Text3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column 3 Text 3"
                                name="./column3Text3"/>
                        </items>
                    </column3>
                    <apps
                        jcr:primaryType="nt:unstructured"
                        jcr:title="App Downloads"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <appsTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Apps Section Title"
                                name="./appsTitle"/>
                            <googlePlayImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="Google Play Badge"
                                fileNameParameter="./googlePlayImage"
                                fileReferenceParameter="./googlePlayImageRef"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./googlePlayImage"
                                title="Upload Google Play Badge"/>
                            <googlePlayLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Google Play Link"
                                name="./googlePlayLink"
                                rootPath="/content"/>
                            <appStoreImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="App Store Badge"
                                fileNameParameter="./appStoreImage"
                                fileReferenceParameter="./appStoreImageRef"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./appStoreImage"
                                title="Upload App Store Badge"/>
                            <appStoreLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="App Store Link"
                                name="./appStoreLink"
                                rootPath="/content"/>
                        </items>
                    </apps>
                    <social
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Social Media"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <instagramLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Instagram Link"
                                name="./instagramLink"
                                rootPath="/content"/>
                            <youtubeLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="YouTube Link"
                                name="./youtubeLink"
                                rootPath="/content"/>
                            <facebookLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Facebook Link"
                                name="./facebookLink"
                                rootPath="/content"/>
                            <sdicLogo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                fieldLabel="SDIC Logo"
                                fileNameParameter="./sdicLogo"
                                fileReferenceParameter="./sdicLogoRef"
                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                multiple="{Boolean}false"
                                name="./sdicLogo"
                                title="Upload SDIC Logo"/>
                        </items>
                    </social>
                    <copyright
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Copyright"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <copyrightText1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Copyright Text 1"
                                name="./copyrightText1"/>
                            <copyrightText2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Copyright Text 2"
                                name="./copyrightText2"/>
                        </items>
                    </copyright>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
