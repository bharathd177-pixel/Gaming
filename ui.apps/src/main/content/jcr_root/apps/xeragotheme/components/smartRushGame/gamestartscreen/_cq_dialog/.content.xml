<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                	<brandLogo
			      		jcr:primaryType="nt:unstructured"
			      		sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
			      		enableNextGenDynamicMedia="{Boolean}true"
			      		fieldLabel="Citi Log"
			      		fileNameParameter="./logoName"
			      		fileReferenceParameter="./logoReference"
			      		mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
			      		name="./brandLogo"/>
                	<gameTitle
						jcr:primaryType="nt:unstructured"             				
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				      	fieldLabel="Game Title"
				      	fieldDescription="Enter Game Title"
				      	name="./gameTitle"/>
				    <gameSubTitle
						jcr:primaryType="nt:unstructured"             				
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				      	fieldLabel="Game Sub Title"
				      	fieldDescription="Enter Game Sub Title"
				      	name="./gameSubTitle"/>
				    <startButtonText
						jcr:primaryType="nt:unstructured"             				
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				      	fieldLabel="Start Button Text"
				      	fieldDescription="Enter Start Button Text"
				      	name="./startButtonText"/>
				    <tncButtonText
						jcr:primaryType="nt:unstructured"             				
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				      	fieldLabel="Tnc Button Text"
				      	fieldDescription="Enter Tnc Button Text"
				      	name="./tncButtonText"/>
				    <tncButtonUrl jcr:primaryType="nt:unstructured"
					    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
					    fieldLabel="Tnc Button Url"
					    fieldDescription="Enter the Url for Tnc Button"
					    rootPath="/content"
					    name="./tncButtonUrl"/>
					<howToPlayButtonText
						jcr:primaryType="nt:unstructured"             				
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				      	fieldLabel="How To Play Button Text"
				      	fieldDescription="Enter How To Play Button Text"
				      	name="./howToPlayButtonText"/>
				    <howToPlayButtonUrl jcr:primaryType="nt:unstructured"
					    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
					    fieldLabel="How To Play Button Url"
					    fieldDescription="Enter the Url for How To Play Button"
					    rootPath="/content"
					    name="./howToPlayButtonUrl"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>